  __1:(pts.El pts.var (congruent n n1 p)) ->
  __2:(pts.El pts.var (congruent m m1 p)) ->
  pts.El pts.var (congruent (matita_arithmetics_nat.times n m) (matita_arithmetics_nat.times n1 m1) p)
