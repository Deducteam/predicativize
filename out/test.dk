          (pts.S (pts.S_N pts.0_N) ?1)
          (pts.Union (pts.S (pts.S_N pts.0_N) ?3) (pts.S (pts.S_N pts.0_N) ?4))))
    (pts.Prod
       (pts.M (pts.S_N pts.0_N) (pts.S (pts.S_N pts.0_N) ?1))
       (pts.M
          (pts.S_N pts.0_N)
