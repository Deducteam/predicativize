test_def1 :
  ?2:pts.Lvl ->
  ?1:pts.Lvl ->
  pts.El
    (pts.M (pts.S_N pts.0_N) (pts.Union (pts.S (pts.S_N pts.0_N) ?1) (pts.S (pts.S_N pts.0_N) ?2)))
    (pts.Prod
       (pts.M (pts.S_N pts.0_N) (pts.S (pts.S_N pts.0_N) ?1))
       (pts.M (pts.S_N pts.0_N) (pts.S (pts.S_N pts.0_N) ?2))
       (pts.u (pts.M pts.0_N (pts.S pts.0_N ?1)))
       (_ => pts.u (pts.M pts.0_N (pts.S pts.0_N ?2)))).


